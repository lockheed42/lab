<script>
    $(function() {
        var b = $(bbs_tid).val();
        var f = $(".common");
        var c = $(".common .comment-list");
        var a = $(".common #bbComment .emoji-box");
        var e;
        $(".m-layer").hide();
        enterbbs.href = "http://bbs.005.tv/thread-" + b + "-1-1.html";

        function d() {
            $.ajax({
                url: "http://bbs.005.tv/source/plugin/gact/gact.php?actions=ReplyPostsList&tid=" + b + "&page=1&limit=10",
                type: "get",
                dataType: "jsonp",
                jsonp: "callback",
                jsonpCallback: "jsonpcallback",
                success: function(k) {
                    $.each(k.comment, function(l, m) {
                        if (!m.is_vip) {
                            c.append('<div class="list-item reply-wrap "> <div class="user-face"><a href="' + m.userUrl + '" target="_blank"><img src="' + m.faceImg + '" alt=""></a></div> <div class="con "> <div class="user"><a href="' + m.userUrl + '" target="_blank" class="name ">' + m.userName + '</a></div> <p class="text">' + userContent(m.content) + '</p> <div class="info"><span class="floor">' + m.position + '楼</span><span class="time">' + m.date + '</span><span class="like "><i></i><span>' + m.goods + '</span></span><p style="display: none">' + m.pid + '</p><span class="hate"><i></i><span>' + m.bad + '</span></span><span class="reply btn-hover">回复</span></div> <div class="reply-box"></div> <div class="paging-box"></div> </div> </div>')
                        } else {
                            c.append('<div class="list-item reply-wrap "> <div class="user-face"><a href="' + m.userUrl + '" target="_blank"><img src="' + m.faceImg + '" alt=""></a><div class="text-content"><p>' + m.produce + '</p><i></i></div></div> <div class="con "> <div class="user"><a href="' + m.userUrl + '" target="_blank" class="name ">' + m.userName + '</a><i class="level"></i><span>' + m.produce + '</span></div><p class="text">' + userContent(m.content) + '</p> <div class="info"><span class="floor">' + m.position + '楼</span><span class="time">' + m.date + '</span><span class="like "><i></i><span>' + m.goods + '</span></span><p style="display: none">' + m.pid + '</p><span class="hate"><i></i><span>' + m.bad + '</span></span><span class="reply btn-hover">回复</span></div> <div class="reply-box"></div> <div class="paging-box"></div> </div> </div>')
                        }
                    });
                    $(".common .comment-list").after(k.page_css)
                }
            });
            $.ajax({
                url: "http://bbs.005.tv/source/plugin/gact/gact.php?actions=getUid&tid=123",
                type: "get",
                dataType: "jsonp",
                jsonp: "callback",
                jsonpCallback: "jsonpcallback",
                success: function(k) {
                    if (k.errorno === 0) {
                        if (k.headerImg_big === undefined) {
                            d()
                        } else {
                            f.find(".comment-send").prepend('<div class="user-face"><div class="text-content" id="userInfo" style="display: block;min-width: 95px;bottom: -82px;z-index: 150;"><p><a href="http://bbs.005.tv/home.php?mod=space&do=notice&view=mypost&type=at" target="_blank">您有' + k.notiCount + '条信息未读取。</a></p><i style="top:-12px;border:2px solid #aaa;border-bottom: none;border-right: none;"></i><b style="position: absolute;top: -4px;right: 3px;font-size: 1.5em;color: #555;cursor: pointer" onclick="closeInfo()">x</b></div><a target="_blank" href="' + k.bbs_tv + '" onclick="closeInfo()"><img class="user-head" src="' + k.headerImg_big + '"></a></div>');
                            e = k.headerImg_big;
                            if (document.getElementById("div0") != null) {
                                div0.style.display = "none"
                            }
                            if (k.notiCount === 0) {
                                closeInfo()
                            }
                            g()
                        }
                    } else {
                        f.find(".comment-send").prepend('<a target="_blank" href="http://usr.005.tv" style="float: left;margin: 7px 0 0 5px;position: relative;">您未登录<br/>点击登录</a>');
                        h()
                    }
                },
                error: function() {
                    h()
                }
            });
            $(".bb-comment").on("click", ".comment-send .textarea-container", function(k) {
                k.stopPropagation();
                $(this).addClass("focus")
            });
            f.click(function() {
                $(this).find(".bb-comment .comment-send .textarea-container").eq(0).removeClass("focus");
                $(".list-item .comment-send").remove();
                a.hide()
            });
            f.on("click", ".ipt-txt", function() {
                $(".textarea-container").removeClass("focus");
                $(this).parent().addClass("focus")
            });
            var j = $(".bb-comment .comment-send .comment-emoji");
            var i;

            function g() {
                a.click(function(l) {
                    l.stopPropagation()
                });
                var k;
                j.click(function(l) {
                    l.stopPropagation();
                    a.css("top", $(this).position().top + 30);
                    a.show();
                    k = 1
                });
                f.on("click", ".bb-comment .comment-list .comment-send .comment-emoji", function(l) {
                    l.stopPropagation();
                    a.css("top", ($(this).parents(".list-item").position().top + $(this).position().top + 30));
                    a.show();
                    k = 0
                });
                f.find(".emoji-box .emoji-wrap a").click(function(m) {
                    m.stopPropagation();
                    if (k) {
                        var n = $(".common .comment-send .ipt-txt");
                        n.eq(0).get(0).value += $(this).text();
                        n.eq(0).focus()
                    } else {
                        var l = $(".common .comment-list .list-item .comment-send .ipt-txt");
                        l.get(0).value += $(this).text();
                        l.focus()
                    }
                });
                c.on("click", ".list-item .info .reply", function(m) {
                    m.stopPropagation();
                    var l = $(".list-item .comment-send");
                    var n = $(this);
                    i = n.parent().prevAll(".user").children(".name").html();
                    if (l) {
                        l.remove()
                    }
                    n.parents(".con").append('<div class="comment-send "><div class="user-face">' + '<img class="user-head" src="' + e + '">' + "</div>" + '<div class="textarea-container focus">' + '<i class="ipt-arrow"></i>' + '<textarea cols="80" name="msg" minlength="1" maxlength="1000" rows="5" placeholder="请输入文字..." class="ipt-txt">回复 @' + i + "：</textarea>" + '<button type="submit" class="comment-submit">发表评论</button>' + "</div>" + '<div class="comment-emoji"><i class="face"></i><span class="text">表情</span></div>' + "</div>")
                });
                $(".comment-submit").click(function() {
                    if ($(this).prev().val().length != 0) {
                        var l = $(this).prev(".ipt-txt").val();
                        $(this).prev().val("");
                        $.ajax({
                            url: "http://bbs.005.tv/source/plugin/gact/gact.php?actions=BackPosts&message=" + l + "&tid=" + b,
                            type: "get",
                            dataType: "jsonp",
                            jsonp: "callback",
                            jsonpCallback: "jsonpcallback",
                            success: function(n) {
                                if (n.errorno == 0) {
                                    if (!n.comment[0].is_vip) {
                                        c.prepend('<div class="list-item reply-wrap "> <div class="user-face"><a href="' + n.comment[0].userUrl + '" target="_blank"><img src="' + n.comment[0].faceImg + '" alt=""></a></div> <div class="con "> <div class="user"><a href="' + n.comment[0].userUrl + '" target="_blank" class="name ">' + n.comment[0].userName + '</a></div> <p class="text">' + userContent(n.comment[0].content) + '</p> <div class="info"><span class="floor">' + n.comment[0].position + '楼</span><span class="time">' + n.comment[0].date + '</span><span class="like "><i></i><span>' + n.comment[0].goods + '</span></span><p style="display: none">' + n.comment[0].pid + '</p><span class="hate"><i></i><span>' + n.comment[0].bad + '</span></span><span class="reply btn-hover">回复</span></div> <div class="reply-box"></div> <div class="paging-box"></div> </div> </div>')
                                    } else {
                                        c.prepend('<div class="list-item reply-wrap "> <div class="user-face"><a href="' + n.comment[0].userUrl + '" target="_blank"><img src="' + n.comment[0].faceImg + '" alt=""></a><div class="text-content"><p>' + n.comment[0].produce + '</p><i></i></div></div> <div class="con "> <div class="user"><a href="' + n.comment[0].userUrl + '" target="_blank" class="name ">' + n.comment[0].userName + '</a><i class="level"></i><span>' + n.comment[0].produce + '</span></div> <p class="text">' + userContent(n.comment[0].content) + '</p> <div class="info"><span class="floor">' + n.comment[0].position + '楼</span><span class="time">' + n.comment[0].date + '</span><span class="like "><i></i><span>' + n.comment[0].goods + '</span></span><p style="display: none">' + n.comment[0].pid + '</p><span class="hate"><i></i><span>' + n.comment[0].bad + '</span></span><span class="reply btn-hover">回复</span></div> <div class="reply-box"></div> <div class="paging-box"></div> </div> </div>')
                                    }
                                } else if(n.errorno == 88){
                                        alert("您的账号未验证绑定手机，暂时不能进行评论，请进入论坛进行手机绑定操作！");
