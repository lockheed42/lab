  var webWidth = 736;
    function changeSwiper(obj) {
        var _width = jQuery(window).width();
        var _is = _width <= webWidth;
        var _this = jQuery(obj);
        var arr = new Array();
        var ahtml = '';
        _this.find('li').each(function (e) {
            arr[e] = jQuery(this).html();
        });
        var arrlen = arr.length;
        var len = 0;
        if (_is) {
            //len = arrlen / 2 + arrlen % 2;
			if(arrlen % 2>0){
				len = parseInt(arrlen / 2)+1;
			}else{
				len = parseInt(arrlen / 2);
			}
        } else {
            //len = arrlen / 5 + arrlen % 5;
			if(arrlen % 5 >0){
				len = parseInt(arrlen / 5)+1;
			}else{
				len = parseInt(arrlen / 5);
			}
        }		
        var j = 0;
        for (var i = 0; i < len; i++) {
            var ihtml = '';
            if (_is) {
                ihtml = '<li>' + arr[j] + '</li><li>' + arr[++j] + '</li>';
            } else {
                ihtml = '<li>' + arr[j] + '</li><li>' + arr[++j] + '</li><li>' + arr[++j] + '</li><li>' + arr[++j] + '</li><li>' + arr[++j] + '</li>';
            }
			ahtml += '<div class="swiper-slide"><ul class="m_pic clear">' + ihtml + '</ul></div>';	
            j++;
        }
        _this.find('.swiper-wrapper').html(ahtml);
    }
    function changeSwiper2(obj) {
        var _width = jQuery(window).width();
        var _is = _width <= webWidth;
        var _this = jQuery(obj);
        var arr = new Array();
        var ahtml = '';
        _this.find('li').each(function (e) {
            arr[e] = jQuery(this).html();
        });
        var arrlen = arr.length;
        var len = 0;
        if (_is) {
            //len = arrlen / 2 + arrlen % 2;
			if(arrlen % 2>0){
				len = parseInt(arrlen / 2)+1;
			}else{
				len = parseInt(arrlen / 2);
			}
        } else {
            //len = arrlen / 5 + arrlen % 5;
			if(arrlen % 8 >0){
				len = parseInt(arrlen / 8)+1;
			}else{
				len = parseInt(arrlen / 8);
			}
        }		
        var j = 0;
        for (var i = 0; i < len; i++) {
            var ihtml = '';
            if (_is) {
                ihtml = '<li class="fj_li">' + arr[j] + '</li><li class="fj_li">' + arr[++j] + '</li>';
            } else {
                ihtml = '<li class="fj_li">' + arr[j] + '</li><li class="fj_li">' + arr[++j] + '</li><li class="fj_li">' + arr[++j] + '</li><li class="fj_li">' + arr[++j] + '</li><li class="fj_li">' + arr[++j] + '</li><li class="fj_li">' + arr[++j] + '</li><li class="fj_li">' + arr[++j] + '</li><li class="fj_li">' + arr[++j] + '</li>';
            }
			ahtml += '<div class="swiper-slide"><ul class="m_pic clear">' + ihtml + '</ul></div>';	
            j++;
        }
        _this.find('.swiper-wrapper').html(ahtml);
    }
jQuery(function() {
	jQuery(".closed").click(
	  function(){
		  jQuery(".topimg").slideUp();
	});
	// 收缩展开效果
	   jQuery(".adminico").click(function(){
		 jQuery(".adminmenu").animate({height: 'toggle', opacity: 'toggle'}, "slow");
		   },function(){
		jQuery(".adminmenu").animate({height: 'toggle', opacity: 'toggle'}, "slow");
		   });
	jQuery(".a_open").click(
	  function(){
		  jQuery(this).hide();
		  jQuery(".aside_lef").slideDown(500);
		  jQuery(".fc_black").show();
		  jQuery(".a_closed").show();
	});
	jQuery(".a_closed").click(
	  function(){
		  jQuery(this).hide();
		  jQuery(".aside_lef").hide();
		  jQuery(".fc_black").hide();
		  jQuery(".a_open").show();
	});
	jQuery(".fc_black").click(
	  function(){
		  jQuery(this).hide();
		  jQuery(".aside_lef").hide();
		  jQuery(".fc_black").hide();
		  jQuery(".a_open").show();
		  jQuery(".a_closed").hide();
	});
	jQuery(".rm_pank .pk_year a").each(function(index){
	   jQuery(this).mouseover(
		  function(){
			  jQuery(".rm_pank .pk_year a").removeClass("current");
			  jQuery(this).addClass("current");
			  jQuery(".rm_pank .rm_main .rm_con").hide();
			  jQuery(".rm_pank .rm_main .rm_con:eq(" + index + ")").show();
	  })
	});
	var headHeight=$(".head_top").height();
	var nav=$("#navf");
	$(window).scroll(function(){
		if($(this).scrollTop()>headHeight){
			nav.addClass("navFix");
		}
		else{
			nav.removeClass("navFix");
		}
	})
	var headHeight2=$(".topimg").height();
	var nav2=jQuery("#topnavf");
    jQuery(window).scroll(function(){
		if($(this).scrollTop()>headHeight2){
			nav2.addClass("topnavFix");
		}
		else{
			nav2.removeClass("topnavFix");
		}
	});
});
