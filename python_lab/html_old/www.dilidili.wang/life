                            $('#so360').show();
                            $('.box3').parent().attr('title','切换至百度搜索');
                        }
                        else
                        {
                            $('.box3').css('background','rgb(252, 171, 103) url("http://www.dilidili.wang/newimages/search_360.png") no-repeat scroll 50% 50% / auto padding-box border-box');
                            $('#so360').hide();
                            $('.search-box').show();
                            $('.box3').parent().attr('title','切换至360搜索');
                        }
                    }
