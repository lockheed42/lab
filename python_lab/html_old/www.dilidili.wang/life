                    {
                        if($('.box3').parent().attr('title') == '切换至360搜索')
                        {
                            $('.box3').css('background','rgb(252, 171, 103) url("http://www.dilidili.wang/newimages/search_baidu.png") no-repeat scroll 50% 50% / auto padding-box border-box');
