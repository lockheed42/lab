                    <script>
                        var outDiv=document.querySelector('ul.topic-preview');
                        var imgCount=outDiv.querySelectorAll('li').length;
                        var imgWidth=outDiv.querySelector('li').clientWidth;
                        //var sliderBarUl=outDiv.parentElement.nextElementSibling.children;
                        for (var i=0;i<imgCount-1;i++) {
                            topic_slider.innerHTML+='<li></li>';
                        }
                        var topicSlider=topic_slider.children;
                        for(var j=0;j<imgCount;j++) {
                            topicSlider[j].index=j;
                            topicSlider[j].onclick=function () {
                                toMoveLeft(this);
                            }
                        }
                        var imgIndex=1;
                        //var outDivMarginLeft=outDiv.style.marginLeft;
                        function toMoveLeft(ele){
                            if(arguments.length===0){
                                outDiv.style.marginLeft='-'+imgIndex*imgWidth+'px';
                                topic_slider.querySelector('li.on').className='';
                                topic_slider.querySelectorAll('li')[imgIndex].className='on';
                                imgIndex++;
                                if (imgIndex===imgCount) {
                                        imgIndex=0;
                                }
                            }
                            else{
                                outDiv.style.marginLeft='-'+imgWidth*ele.index+'px';
                                topic_slider.querySelector('li.on').className='';
                                ele.className='on';
                                imgIndex=ele.index;
                            }
                        }
                        var myTimer=setInterval(toMoveLeft, 4000);
                        outDiv.onmouseover=function () {
                            clearInterval(myTimer);
                        }
                        outDiv.onmouseout=function () {
                            myTimer=setInterval(toMoveLeft, 4000);
                        }

                        document.addEventListener('visibilitychange',function () {
                            document.hidden?clearInterval(myTimer):myTimer=setInterval(toMoveLeft, 4000);
                        })
                    </script>
