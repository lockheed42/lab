<script>
    var eleArray=document.querySelectorAll('main.dili>ul>li>div.update-content>h4>a');
    var contentArray=[];
    for(var i=0;i<eleArray.length;i++)
    {
        a(eleArray[i].innerText,i);
        contentArray.push(eleArray[i].innerText);
    }

    function a(item,index)
    {
        for(var j=0;j<contentArray.length;j++)
        {
            if(item==contentArray[j])
            {
                $(eleArray[index]).parents('li').remove();
            }
        }
    }
</script>
