            <script>
                $(function() {
                    var count11 = 0
                    setInterval(function() {
                        if (count11 == 0) {
                            $("a[href='http://www.masadora.net/app.htm?pttag=dilidili']").fadeOut(800)
                            $("a[href='http://moecolle.masadora.net/?pttag=dilidili']").fadeIn(800)
                            count11++
                        } else {
                            $("a[href='http://www.masadora.net/app.htm?pttag=dilidili']").fadeIn(800)
                            $("a[href='http://moecolle.masadora.net/?pttag=dilidili']").fadeOut(800)
                            count11 = 0
                        }
                    }, 3000)
                })
            </script>
