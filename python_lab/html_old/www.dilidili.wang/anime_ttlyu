                count++
                if(count>0){
                    $('.roldiv').stop(true,false).animate({'left':0+'%'})
                    count=-5
                }
                else{
                    $('.roldiv').stop(true,false).animate({'left':count*49.5+'%'})
                }
            })
            $(".btnr").click(function(){
                count--
                if(count<-4){
                    count=0
                    $('.roldiv').stop(true,false).animate({'left':count*49.5+'%'})
                }else{
                    $('.roldiv').stop(true,false).animate({'left':count*49.5+'%'})
                }
            })

            $(".zhangkai").click(function(){
                $(".zknum").stop(true,false).slideToggle(800)
            })



            var timer=setInterval(function(){
                count--
                if(count<-4){
                    count=0
                }
                $('.roldiv').stop(true,false).animate({'left':count*49.5+'%'})

            },3000);


