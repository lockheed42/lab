            f.on("click", ".bottom-page a", function() {
                $(this).addClass("current").siblings().removeClass("current");
                var k = $(this).text();
                $.ajax({
                    url: "http://bbs.005.tv/source/plugin/gact/gact.php?actions=ReplyPostsList&tid=" + b + "&page=" + k + "&limit=10",
                    type: "get",
                    dataType: "jsonp",
                    jsonp: "callback",
                    jsonpCallback: "jsonpcallback",
                    success: function(l) {
                        if (l.errorno == 0) {
                            c.empty();
                            $.each(l.comment, function(m, n) {
                                if (!n.is_vip) {
                                    c.append('<div class="list-item reply-wrap "> <div class="user-face"><a href="' + n.userUrl + '" target="_blank"><img src="' + n.faceImg + '" alt=""></a></div> <div class="con "> <div class="user"><a href="' + n.userUrl + '" target="_blank" class="name ">' + n.userName + '</a></div> <p class="text">' + userContent(n.content) + '</p> <div class="info"><span class="floor">' + n.position + '楼</span><span class="time">' + n.date + '</span><span class="like "><i></i><span>' + n.goods + '</span></span><p style="display: none">' + n.pid + '</p><span class="hate"><i></i><span>' + n.bad + '</span></span><span class="reply btn-hover">回复</span></div> <div class="reply-box"></div> <div class="paging-box"></div> </div> </div>')
                                } else {
                                    c.append('<div class="list-item reply-wrap "> <div class="user-face"><a href="' + n.userUrl + '" target="_blank"><img src="' + n.faceImg + '" alt=""></a><div class="text-content"><p>' + n.produce + '</p><i></i></div></div> <div class="con "> <div class="user"><a href="' + n.userUrl + '" target="_blank" class="name ">' + n.userName + '</a><i class="level"></i><span>' + n.produce + '</span></div><p class="text">' + userContent(n.content) + '</p> <div class="info"><span class="floor">' + n.position + '楼</span><span class="time">' + n.date + '</span><span class="like "><i></i><span>' + n.goods + '</span></span><p style="display: none">' + n.pid + '</p><span class="hate"><i></i><span>' + n.bad + '</span></span><span class="reply btn-hover">回复</span></div> <div class="reply-box"></div> <div class="paging-box"></div> </div> </div>')
                                }
                            });
                            $(".common .bottom-page").remove();
                            $(".common .comment-list").after(l.page_css)
                        } else {}
                    }
                })
            });
            $(".tabs-order ul li").click(function() {
                $(this).addClass("on").siblings().removeClass("on")
            })
        }
        d()
    });

    function closeInfo() {
        userInfo.style.display = "none"
    }

    function userContent(b) {
        if (/@(.*)：/g.test(b)) {
            var a = /@(.*)：/g.exec(b);
            a = b.replace(a[0], '<span style="color:#00a1d6;cursor:pointer">' + a[0] + "</span>");
            return a
        } else {
            return b
        }
    }
    if (bbComment.clientWidth < 270) {
        $(".emoji-box").css("width", "270px");
        bbComment.addEventListener("DOMNodeInserted", function() {
            $("#bbComment .comment-send").not($("#bbComment .comment-send").eq(0)).css("transform", "translateX(-80px)").css("width", "270px")
        })
    };

