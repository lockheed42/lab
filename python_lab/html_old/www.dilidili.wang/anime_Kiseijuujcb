<script src="/newjs/jquery-3-2-1.js"></script>
<script>

    $(function () {


        var bbsId=$(bbs_tid).val();//文章ID
        var oCommon=$('.common');//整个评论部分
        var commonList=$('.common .comment-list');//评论楼层列表
        var emojiBox=$('.common #bbComment .emoji-box');//颜文字表情框
        var myFace;
        $('.m-layer').hide();
        enterbbs.href='http://bbs.005.tv/thread-'+bbsId+'-1-1.html';
        function reLoad() {

            //load commemt  获取评论列表第一页
            $.ajax({
                url:"http://bbs.005.tv/source/plugin/gact/gact.php?actions=ReplyPostsList&tid="+bbsId+"&page=1&limit=10",
                type:"get",
                dataType:"jsonp",
//        anync: false,
                jsonp: "callback",//传递给请求处理程序或页面的，用以获得jsonp回调函数名的参数名(默认为:callback)
                jsonpCallback: "jsonpcallback",//自定义的jsonp回调函数名称，
                success:function (res) {//alert(JSON.stringify(res))
                    //alert(res.comment[0].userName)
                    $.each(res.comment,function (i,item) {
                        if(!item.is_vip){
                            commonList.append('<div class="list-item reply-wrap "> <div class="user-face"><a href="'+item.userUrl+'" target="_blank"><img src="'+item.faceImg+'" alt=""></a></div> <div class="con "> <div class="user"><a href="'+item.userUrl+'" target="_blank" class="name ">'+item.userName+'</a></div> <p class="text">'+userContent(item.content)+'</p> <div class="info"><span class="floor">'+item.position+'楼</span><span class="time">'+item.date+'</span><span class="like "><i></i><span>'+item.goods+'</span></span><p style="display: none">'+item.pid+'</p><span class="hate"><i></i><span>'+item.bad+'</span></span><span class="reply btn-hover">回复</span></div> <div class="reply-box"></div> <div class="paging-box"></div> </div> </div>');
                        }else{
                            commonList.append('<div class="list-item reply-wrap "> <div class="user-face"><a href="'+item.userUrl+'" target="_blank"><img src="'+item.faceImg+'" alt=""></a><div class="text-content"><p>'+item.produce+'</p><i></i></div></div> <div class="con "> <div class="user"><a href="'+item.userUrl+'" target="_blank" class="name ">'+item.userName+'</a><i class="level"></i><span>'+item.produce+'</span></div><p class="text">'+userContent(item.content)+'</p> <div class="info"><span class="floor">'+item.position+'楼</span><span class="time">'+item.date+'</span><span class="like "><i></i><span>'+item.goods+'</span></span><p style="display: none">'+item.pid+'</p><span class="hate"><i></i><span>'+item.bad+'</span></span><span class="reply btn-hover">回复</span></div> <div class="reply-box"></div> <div class="paging-box"></div> </div> </div>');
                        }
                    });
                    $('.common .comment-list').after(res.page_css);
                }
            });

//判断是否登录
            $.ajax({
                url:"http://bbs.005.tv/source/plugin/gact/gact.php?actions=getUid&tid=123",
                type:"get",
//        anync: false,
                dataType:"jsonp",
                jsonp: "callback",//传递给请求处理程序或页面的，用以获得jsonp回调函数名的参数名(默认为:callback)
                jsonpCallback: "jsonpcallback",//自定义的jsonp回调函数名称，
                success:function (res) {
                    if(res.errorno===0){
                        if(res.headerImg_big===undefined){
                            reLoad();
                        }else{
                            oCommon.find('.comment-send').prepend('<div class="user-face"><div class="text-content" id="userInfo" style="display: block;min-width: 95px;bottom: -82px;z-index: 150;"><p><a href="http://bbs.005.tv/home.php?mod=space&amp;do=notice&amp;view=mypost&amp;type=at" target="_blank">您有'+res.notiCount+'条信息未读取。</a></p><i style="top:-12px;border:2px solid #aaa;border-bottom: none;border-right: none;"></i><b style="position: absolute;top: -4px;right: 3px;font-size: 1.5em;color: #555;cursor: pointer" onclick="closeInfo()">x</b></div><a target="_blank" href="'+res.bbs_tv+'" onclick="closeInfo()"><img class="user-head" src="'+res.headerImg_big+'"></a></div>');
                            myFace=res.headerImg_big;//保存本人头像url
                            if(res.notiCount===0){   //信息提示是否显示
                                closeInfo();
                            }
                            userLogin();
                        }
                    }else{
                        oCommon.find('.comment-send').prepend('<a target="_blank" href="http://usr.005.tv" style="position:absolute;top:75px;left:14px">您未登录<br/>点击登录</a>')
                        userNoLogin();
                    }
                },
                error:function () {
                    userNoLogin();
                }
            });



            //焦点样式
            $('.bb-comment').on('click','.comment-send .textarea-container',function (event) {
                event.stopPropagation();
                $(this).addClass('focus');
            });
            oCommon.click(function () {
                $(this).find('.bb-comment .comment-send .textarea-container').eq(0).removeClass('focus');
                $('.list-item .comment-send').remove();
                emojiBox.hide();
            });
            oCommon.on('click','.ipt-txt',function () {
                $('.textarea-container').removeClass('focus');
                $(this).parent().addClass('focus');
            });

            var emojiBtn=$('.bb-comment .comment-send .comment-emoji');
            var huifuPeople;
            function userLogin() {

                //emoji-box
                emojiBox.click(function (event) {
                    event.stopPropagation();
                });

                var emojiFlag;
                emojiBtn.click(function (event) {
                    event.stopPropagation();
                    emojiBox.css('top',$(this).position().top+30);
                    emojiBox.show();
                    emojiFlag=1;
                });

                oCommon.on('click','.bb-comment .comment-list .comment-send .comment-emoji',function (event) {
                    event.stopPropagation();
                    emojiBox.css('top',($(this).parents('.list-item').position().top+$(this).position().top+30));
                    emojiBox.show();
                    emojiFlag=0;
                });

                //emoji选择
                oCommon.find('.emoji-box .emoji-wrap a').click(function (event) {
                    event.stopPropagation();
                    if(emojiFlag){
                        var thisTextBox=$('.common .comment-send .ipt-txt');
                        thisTextBox.eq(0).get(0).value+=$(this).text();
                        thisTextBox.eq(0).focus();
                    }else{
                        var thisTextBox2=$('.common .comment-list .list-item .comment-send .ipt-txt');
                        thisTextBox2.get(0).value+=$(this).text();
                        thisTextBox2.focus();
                    }
                });


                //回复框show
                commonList.on('click','.list-item .info .reply',function (event) {
                    event.stopPropagation();
                    var listItemCommentSend=$('.list-item .comment-send');
                    var thisEle=$(this);
                    huifuPeople=thisEle.parent().prevAll('.user').children('.name').html();
                    if(listItemCommentSend){listItemCommentSend.remove()};
                    thisEle.parents('.con').append(
                            '<div class="comment-send "><div class="user-face">' +
                            '<img class="user-head" src="'+myFace+'">' +
                            '</div>' +
                            '<div class="textarea-container focus">' +
                            '<i class="ipt-arrow"></i>' +
                            '<textarea cols="80" name="msg" minlength="1" maxlength="1000" rows="5" placeholder="请输入文字..." class="ipt-txt">回复 @'+huifuPeople+'：</textarea>' +
                            '<button type="submit" class="comment-submit">发表评论</button>' +
                            '</div>' +
                            '<div class="comment-emoji"><i class="face"></i><span class="text">表情</span></div>' +
                            '</div>');
                });



                //直接评论
                $('.comment-submit').click(function () {
                    if($(this).prev().val().length!=0) {
                        var content = $(this).prev('.ipt-txt').val();
                        $(this).prev().val('');
                        $.ajax({
                            url:"http://bbs.005.tv/source/plugin/gact/gact.php?actions=BackPosts&message="+content+"&tid="+bbsId,
                            type:"get",
                            //date:'&message='+content+'tid&='+bbsId,
                            dataType:"jsonp",
                            jsonp: "callback",//传递给请求处理程序或页面的，用以获得jsonp回调函数名的参数名(默认为:callback)
                            jsonpCallback: "jsonpcallback",//自定义的jsonp回调函数名称，
                            success:function (res) {
                                if(res.errorno==0){
                                    if(!res.comment[0].is_vip){
                                        commonList.prepend('<div class="list-item reply-wrap "> <div class="user-face"><a href="'+res.comment[0].userUrl+'" target="_blank"><img src="'+res.comment[0].faceImg+'" alt=""></a></div> <div class="con "> <div class="user"><a href="'+res.comment[0].userUrl+'" target="_blank" class="name ">'+res.comment[0].userName+'</a></div> <p class="text">'+userContent(res.comment[0].content)+'</p> <div class="info"><span class="floor">'+res.comment[0].position+'楼</span><span class="time">'+res.comment[0].date+'</span><span class="like "><i></i><span>'+res.comment[0].goods+'</span></span><p style="display: none">'+res.comment[0].pid+'</p><span class="hate"><i></i><span>'+res.comment[0].bad+'</span></span><span class="reply btn-hover">回复</span></div> <div class="reply-box"></div> <div class="paging-box"></div> </div> </div>');
                                    }else{
                                        commonList.prepend('<div class="list-item reply-wrap "> <div class="user-face"><a href="'+res.comment[0].userUrl+'" target="_blank"><img src="'+res.comment[0].faceImg+'" alt=""></a><div class="text-content"><p>'+res.comment[0].produce+'</p><i></i></div></div> <div class="con "> <div class="user"><a href="'+res.comment[0].userUrl+'" target="_blank" class="name ">'+res.comment[0].userName+'</a><i class="level"></i><span>'+res.comment[0].produce+'</span></div> <p class="text">'+userContent(res.comment[0].content)+'</p> <div class="info"><span class="floor">'+res.comment[0].position+'楼</span><span class="time">'+res.comment[0].date+'</span><span class="like "><i></i><span>'+res.comment[0].goods+'</span></span><p style="display: none">'+res.comment[0].pid+'</p><span class="hate"><i></i><span>'+res.comment[0].bad+'</span></span><span class="reply btn-hover">回复</span></div> <div class="reply-box"></div> <div class="paging-box"></div> </div> </div>');
                                    }
//                            alert('评论成功！');
                                }else{
                                    alert(res.msg);
                                }
                            },
                            error:function () {
                                alert('评论失败！拥有非法字符或提交失败');
                            }
                        })
                    }else{
                        $('.m-layer').show();
                        setTimeout(hidesm,3000);
                        function hidesm() {
                            $('.m-layer.m_layer.m-error').hide();
                        }
                    }
                });


                //回复提交
                commonList.on('click','.list-item .comment-send .textarea-container button',function () {
                    if($(this).prev().val().length!=0) {
                        var content = $(this).prev('.ipt-txt').val();
                        $(this).prev().val('回复 @'+huifuPeople+'：');
                        var atPeople=/@(.*)：/g.exec(content);
                        atPeople=atPeople[1].replace('@','');
                        $.ajax({
                            url:"http://bbs.005.tv/source/plugin/gact/gact.php?actions=BackPosts&message="+content+"&tid="+bbsId+"&at="+atPeople,
                            type:"get",
                            dataType:"jsonp",
                            jsonp: "callback",//传递给请求处理程序或页面的，用以获得jsonp回调函数名的参数名(默认为:callback)
                            jsonpCallback: "jsonpcallback",//自定义的jsonp回调函数名称，
                            success:function (res) {
                                if(res.errorno==0){
                                    if(!res.comment[0].is_vip){
                                        commonList.prepend('<div class="list-item reply-wrap "> <div class="user-face"><a href="'+res.comment[0].userUrl+'" target="_blank"><img src="'+res.comment[0].faceImg+'" alt=""></a></div> <div class="con "> <div class="user"><a href="'+res.comment[0].userUrl+'" target="_blank" class="name ">'+res.comment[0].userName+'</a></div> <p class="text">'+userContent(res.comment[0].content)+'</p> <div class="info"><span class="floor">'+res.comment[0].position+'楼</span><span class="time">'+res.comment[0].date+'</span><span class="like "><i></i><span>'+res.comment[0].goods+'</span></span><p style="display: none">'+res.comment[0].pid+'</p><span class="hate"><i></i><span>'+res.comment[0].bad+'</span></span><span class="reply btn-hover">回复</span></div> <div class="reply-box"></div> <div class="paging-box"></div> </div> </div>');
                                    }else{
                                        commonList.prepend('<div class="list-item reply-wrap "> <div class="user-face"><a href="'+res.comment[0].userUrl+'" target="_blank"><img src="'+res.comment[0].faceImg+'" alt=""></a><div class="text-content"><p>'+res.comment[0].produce+'</p><i></i></div></div> <div class="con "> <div class="user"><a href="'+res.comment[0].userUrl+'" target="_blank" class="name ">'+res.comment[0].userName+'</a><i class="level"></i><span>'+res.comment[0].produce+'</span></div> <p class="text">'+userContent(res.comment[0].content)+'</p> <div class="info"><span class="floor">'+res.comment[0].position+'楼</span><span class="time">'+res.comment[0].date+'</span><span class="like "><i></i><span>'+res.comment[0].goods+'</span></span><p style="display: none">'+res.comment[0].pid+'</p><span class="hate"><i></i><span>'+res.comment[0].bad+'</span></span><span class="reply btn-hover">回复</span></div> <div class="reply-box"></div> <div class="paging-box"></div> </div> </div>');
                                    }
                                }else{
                                    alert(res.msg+'，由于近期维护，请重新上传您的头像（￣▽￣）。');
                                }
                            },
                            error:function (res) {
                                alert('评论失败！拥有非法字符或提交失败');
                            }
                        })
                    }else{
                        $('.m-layer').show();
                        setTimeout(hidesm,3000);
                        function hidesm() {
                            $('.m-layer.m_layer.m-error').hide();
                        }
                    }
                });



                //点赞

                oCommon.on('click','.comment-list .list-item .con .info .like',function () {
                    $(this).append('<p class="flag" style="display: none;">0</p>');
                    if($(this).find('.flag').eq(0).text()==0){
                        $(this).find('.flag').eq(0).text('1');
                        $(this).addClass('liked');
                        $(this).find('span').text(parseInt($(this).find('span').text())+1);
                        var pid=$(this).next().text();

                        $.ajax({
                            url:"http://bbs.005.tv/source/plugin/gact/gact.php?actions=postreview&tid="+bbsId+"&pid="+pid+"&do=support",
                            type:"get",
                            dataType:"jsonp",
                            jsonp: "callback",//传递给请求处理程序或页面的，用以获得jsonp回调函数名的参数名(默认为:callback)
                            jsonpCallback: "jsonpcallback",//自定义的jsonp回调函数名称，
                            success:function (res) {
                                if(res.errorno!=0){
                                    alert(res.msg);
                                }
                            },
                            error:function (res) {
                                console.log(res.errorno,res.msg)
                            }
                        });
                    }
                });

                oCommon.on('click','.comment-list .list-item .con .info .hate',function () {
                    $(this).append('<p class="flag" style="display: none;">0</p>');
                    if($(this).find('.flag').eq(0).text()==0){
                        $(this).find('.flag').eq(0).text('1');
                        $(this).addClass('hated');
                        $(this).find('span').text(parseInt($(this).find('span').text())+1);
                        var pid=$(this).prev().text();
                        $.ajax({
                            url:"http://bbs.005.tv/source/plugin/gact/gact.php?actions=postreview&tid="+bbsId+"&pid="+pid+"&do=against",
                            type:"get",
                            dataType:"jsonp",
                            jsonp: "callback",//传递给请求处理程序或页面的，用以获得jsonp回调函数名的参数名(默认为:callback)
                            jsonpCallback: "jsonpcallback",//自定义的jsonp回调函数名称，
                            success:function (res) {
                                if(res.errorno!=0){
                                    alert(res.msg+'，由于近期维护，请重新上传您的头像（￣▽￣）。');
                                }
                            },

                            error:function (res) {
                            }
                        })
                    }
                });


            }
            function userNoLogin() {
                //未登录时点击出现的提示信息
                oCommon.find('.comment-send button.comment-submit').click(function () {
                    alert('您还未登录！');
                    window.open('http://usr.005.tv')
                });
                oCommon.on('click','.comment-list .info .reply',function (){
                    alert('您还未登录！');
                    window.open('http://usr.005.tv');
                });
                emojiBtn.click(function () {
                   alert('请登录后再评论！') ;
                });

                //未登录点赞评论是出现提示
                oCommon.on('click','.comment-list .list-item .con .info .like',function () {
                  alert('请登录后再评论！')
                });
                oCommon.on('click','.comment-list .list-item .con .info .hate',function () {
                    alert('请登录后再评论！')
                });
            }
            //页数
            oCommon.on('click','.bottom-page a',function () {
                $(this).addClass('current').siblings().removeClass('current');
                //$('.header-page a').eq($(this).index()).addClass('current').siblings().removeClass('current');
                var $thisPage=$(this).text();

                $.ajax({
                    url:"http://bbs.005.tv/source/plugin/gact/gact.php?actions=ReplyPostsList&tid="+bbsId+"&page="+$thisPage+"&limit=10",
                    type:'get',
                    dataType:"jsonp",
                    jsonp: "callback",//传递给请求处理程序或页面的，用以获得jsonp回调函数名的参数名(默认为:callback)
                    jsonpCallback: "jsonpcallback",//自定义的jsonp回调函数名称，
                    success:function (res) {
                        if(res.errorno==0){
                            commonList.empty();
                            $.each(res.comment,function (i,item) {
                                if(!item.is_vip){
                                    commonList.append('<div class="list-item reply-wrap "> <div class="user-face"><a href="'+item.userUrl+'" target="_blank"><img src="'+item.faceImg+'" alt=""></a></div> <div class="con "> <div class="user"><a href="'+item.userUrl+'" target="_blank" class="name ">'+item.userName+'</a></div> <p class="text">'+userContent(item.content)+'</p> <div class="info"><span class="floor">'+item.position+'楼</span><span class="time">'+item.date+'</span><span class="like "><i></i><span>'+item.goods+'</span></span><p style="display: none">'+item.pid+'</p><span class="hate"><i></i><span>'+item.bad+'</span></span><span class="reply btn-hover">回复</span></div> <div class="reply-box"></div> <div class="paging-box"></div> </div> </div>');
                                }else{
                                    commonList.append('<div class="list-item reply-wrap "> <div class="user-face"><a href="'+item.userUrl+'" target="_blank"><img src="'+item.faceImg+'" alt=""></a><div class="text-content"><p>'+item.produce+'</p><i></i></div></div> <div class="con "> <div class="user"><a href="'+item.userUrl+'" target="_blank" class="name ">'+item.userName+'</a><i class="level"></i><span>'+item.produce+'</span></div><p class="text">'+userContent(item.content)+'</p> <div class="info"><span class="floor">'+item.position+'楼</span><span class="time">'+item.date+'</span><span class="like "><i></i><span>'+item.goods+'</span></span><p style="display: none">'+item.pid+'</p><span class="hate"><i></i><span>'+item.bad+'</span></span><span class="reply btn-hover">回复</span></div> <div class="reply-box"></div> <div class="paging-box"></div> </div> </div>');
                                }
                            });
                            $('.common .bottom-page').remove();
                            $('.common .comment-list').after(res.page_css);
                        }else{
//                    alert('评论失败！'+res.msg)
                        }

                    }
                })
            });

            //切换
            $('.tabs-order ul li').click(function () {
                $(this).addClass('on').siblings().removeClass('on');
            });
        }
        reLoad();
    });

    //closeInfo
    function closeInfo() {
        userInfo.style.display='none';
    }

    //fontColor
    function userContent(str) {
        if (/@(.*)：/g.test(str)){
            var fontColor = /@(.*)：/g.exec(str);
            fontColor=str.replace(fontColor[0],'<span style="color:#00a1d6;cursor:pointer">' + fontColor[0] + '</span>');
            return fontColor;
        }else{
            return str;
        }
    }
</script>
