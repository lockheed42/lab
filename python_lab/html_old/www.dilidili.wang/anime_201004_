                                console.log(m.errorno, m.msg)
                            }
                        })
                    }
                });
                f.on("click", ".comment-list .list-item .con .info .hate", function() {
                    $(this).append('<p class="flag" style="display: none;">0</p>');
                    if ($(this).find(".flag").eq(0).text() == 0) {
                        $(this).find(".flag").eq(0).text("1");
                        $(this).addClass("hated");
                        $(this).find("span").text(parseInt($(this).find("span").text()) + 1);
                        var l = $(this).prev().text();
