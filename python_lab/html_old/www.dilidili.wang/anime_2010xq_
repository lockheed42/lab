    .bb-comment .user-face:hover .text-content {
        display: block;
    }

    .bb-comment .user-face .text-content {
        display: none;
        min-width: 250px;
        position: absolute;
        bottom: 55px;
        left: 0;
        z-index: 220;
        background: #eee;
        border: 2px solid #aaa;
        border-radius: 5px;
    }

    .bb-comment .user-face .text-content p {
        padding: 20px 10px
    }

    .bb-comment .user-face .text-content i {
        position: absolute;
        display: block;
        width: 20px;
        height: 20px;
        bottom: -12px;
        left: 14px;
        border-right: 2px solid #aaa;
        border-bottom: 2px solid #aaa;
        transform: rotate(45deg);
        background: #eee;
    }


    @media screen and (max-width:960px) {
        .common {
            padding: 0 5px;
        }
        .bb-comment .comment-header {
            border: none;
        }
        .bb-comment .comment-header .tabs-order li.on {
            border: none;
        }

    }

