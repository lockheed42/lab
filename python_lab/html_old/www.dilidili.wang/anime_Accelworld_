        jQuery(".xf_news .time_pic .time_con").hide();
        if (week != 0) {
            jQuery(".xf_news .pk_time a:eq(" + (0) + ")").addClass("current");
            jQuery(".xf_news .time_pic .time_con:eq(" + (0) + ")").show();
            new Swiper(".xfswiper"+(0), {
