<script>
var line = ["http://www.maoyun.tv/mdparse/index.php?id=", "http://jx.maoyun.tv/index.php?id=", "http://dilidili.97kn.com/?url="];

var buttons = '<div class="player_switch"><p>无法播放？试试切换线路</p><div>'
for(var i = 0; i < line.length; i++)
{
	iPlus = i + 1;
	var temp = '<button id="' + i + '" onclick="playerSwitch(this.id)">线路' + iPlus + '</button>';
	buttons += temp;
}
playerSwitch = function(lineIndex)
{
    $(".player_main iframe").attr("src",line[lineIndex] + "http://v.youku.com/v_show/id_XMzE2OTE2NjM2.html");
