                                    } else {
                                        c.prepend('<div class="list-item reply-wrap "> <div class="user-face"><a href="' + o.comment[0].userUrl + '" target="_blank"><img src="' + o.comment[0].faceImg + '" alt=""></a><div class="text-content"><p>' + o.comment[0].produce + '</p><i></i></div></div> <div class="con "> <div class="user"><a href="' + o.comment[0].userUrl + '" target="_blank" class="name ">' + o.comment[0].userName + '</a><i class="level"></i><span>' + o.comment[0].produce + '</span></div> <p class="text">' + userContent(o.comment[0].content) + '</p> <div class="info"><span class="floor">' + o.comment[0].position + '楼</span><span class="time">' + o.comment[0].date + '</span><span class="like "><i></i><span>' + o.comment[0].goods + '</span></span><p style="display: none">' + o.comment[0].pid + '</p><span class="hate"><i></i><span>' + o.comment[0].bad + '</span></span><span class="reply btn-hover">回复</span></div> <div class="reply-box"></div> <div class="paging-box"></div> </div> </div>')
                                    }
                                } else if(o.errorno == 88){
                                        alert("您的账号未验证绑定手机，暂时不能进行评论，请进入论坛进行手机绑定操作！");
                                        location.href="http://bbs.005.tv/home.php?mod=spacecp&ac=plugin&id=jzsjiale_sms:home";
                                  } else {
                                    alert(o.msg + "，由于近期维护，请重新上传您的头像（￣▽￣）。")
                                }
                            },
                            error: function(o) {
                                alert("评论失败！拥有非法字符或提交失败")
                            }
                        })
                    } else {
                        $(".m-layer").show();
                        setTimeout(n, 3000);

                        function n() {
                            $(".m-layer.m_layer.m-error").hide()
                        }
                    }
                });
                f.on("click", ".comment-list .list-item .con .info .like", function() {
                    $(this).append('<p class="flag" style="display: none;">0</p>');
                    if ($(this).find(".flag").eq(0).text() == 0) {
                        $(this).find(".flag").eq(0).text("1");
                        $(this).addClass("liked");
                        $(this).find("span").text(parseInt($(this).find("span").text()) + 1);
                        var l = $(this).next().text();
                        $.ajax({
                            url: "http://bbs.005.tv/source/plugin/gact/gact.php?actions=postreview&tid=" + b + "&pid=" + l + "&do=support",
                            type: "get",
