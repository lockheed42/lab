    <script>
        jQuery(".playjs").hide();
        function changeSwiper1(obj) {
            var _width = jQuery(window).width();
            var _is = _width <= webWidth;
            var _this = jQuery(obj);
            var arr = new Array();
            var ahtml = '';
            _this.find('li').each(function (e) {
                arr[e] = jQuery(this).html();
            });
            var arrlen = arr.length;
//		alert(arrlen);
            var len = 0;
            if (_is) {
                if(arrlen % 2>0){
                    len = parseInt(arrlen / 2)+1;
                }else{
                    len = parseInt(arrlen / 2);
                }
            } else {
                if(arrlen % 20 >0){
                    len = parseInt(arrlen / 20)+1;
                }else{
                    len = parseInt(arrlen / 20);
                }
            }
            if($(".xfswiper1 li").length>21){jQuery(".playjs").show();}else{jQuery(".playjs").hide();};
            var j = 0;
            for (var i = 0; i < len; i++) {
                var ihtml = '';
                if (_is) {
                    ihtml = '<li>' + arr[j] + '</li><li>' + arr[++j] + '</li>';
                } else {
                    ihtml = '<li>' + arr[j] + '</li><li>' + arr[++j] + '</li><li>' + arr[++j] + '</li><li>' + arr[++j] + '</li><li>' + arr[++j] + '</li><li>' + arr[++j] + '</li><li>' + arr[++j] + '</li><li>' + arr[++j] + '</li><li>' + arr[++j] + '</li><li>' + arr[++j] + '</li><li>' + arr[++j] + '</li><li>' + arr[++j] + '</li><li>' + arr[++j] + '</li><li>' + arr[++j] + '</li><li>' + arr[++j] + '</li><li>' + arr[++j] + '</li><li>' + arr[++j] + '</li><li>' + arr[++j] + '</li><li>' + arr[++j] + '</li><li>' + arr[++j] + '</li>';
                }
                ahtml += '<div class="swiper-slide"><ul class="clear">' + ihtml + '</ul></div>';
                j++;
            }
            _this.find('.swiper-wrapper').html(ahtml);
        }
        changeSwiper1('.xfswiper0');
        changeSwiper1('.xfswiper1');
        changeSwiper1('.xfswiper2');
        changeSwiper1('.xfswiper3');
        changeSwiper1('.xfswiper4');
        changeSwiper1('.xfswiper5');
        changeSwiper1('.xfswiper6');
        changeSwiper('.swiper3');
        changeSwiper('.swiper5');
        changeSwiper('.swiper6');
        changeSwiper('.swiper7');
        changeSwiper2('.swiper4');
        jQuery(window).resize(function () {
            changeSwiper1('.xfswiper0');
            changeSwiper1('.xfswiper1');
            changeSwiper1('.xfswiper2');
            changeSwiper1('.xfswiper3');
            changeSwiper1('.xfswiper4');
            changeSwiper1('.xfswiper5');
            changeSwiper1('.xfswiper6');
            changeSwiper('.swiper3');
            changeSwiper('.swiper5');
            changeSwiper('.swiper6');
            changeSwiper('.swiper7');
            changeSwiper2('.swiper4');
        });
        var swiper1 = new Swiper('.xfswiper1', {
            nextButton: '.xfswiper-button-next1',
            prevButton: '.xfswiper-button-prev1',
            spaceBetween: 30
        });
        var swiper2 = new Swiper('.xfswiper2', {
            nextButton: '.xfswiper-button-next2',
            prevButton: '.xfswiper-button-prev2',
            spaceBetween: 30
        });
