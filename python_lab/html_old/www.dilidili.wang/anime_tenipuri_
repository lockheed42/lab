                                }
                            },
                            error: function(o) {
                                alert("评论失败！拥有非法字符或提交失败")
                            }
                        })
                    } else {
                        $(".m-layer").show();
                        setTimeout(n, 3000);

                        function n() {
                            $(".m-layer.m_layer.m-error").hide()
                        }
                    }
                });
