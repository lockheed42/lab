                <script>
                    orderSwitch = function(order)
                    {
                    	//alert(order);
                    	if(order == 'list_order_default')
                        {
                    		if(!$('#list_order_default').hasClass('on'))
                    		{
                    			$('#list_order_default').toggleClass('on');$('#list_order_hot').toggleClass('on');
                    			$('#orderTime').show();$('#orderHot').hide();
                            }
                    	}
                    	else
                        {
                            if(!$('#list_order_hot').hasClass('on'))
                    		{
                    			$('#list_order_hot').toggleClass('on');$('#list_order_default').toggleClass('on');
                    			$('#orderHot').show();$('#orderTime').hide();
                            }
                        }
                    }
                </script>
