<script src="http://static.jfrft.com/templets/default/js/swiper.min.js"></script>
<script>
    var mySwiper1 = new Swiper('.swiper1',{
        pagination: '.pagination1',
        paginationClickable: true,
        moveStartThreshold: 100,
        autoplay:4000
    });
    var mySwiper2 = new Swiper('.swiper2',{
        pagination: '.pagination2',
        paginationClickable: true,
        moveStartThreshold: 100
    });
    var webWidth = 736;
    function changeSwiper(obj) {
        var _width = jQuery(window).width();
        var _is = _width <= webWidth;
        var _this = jQuery(obj);
        var arr = new Array();
        var ahtml = '';
        _this.find('li').each(function (e) {
            arr[e] = jQuery(this).html();
        });
        var arrlen = arr.length;
        var len = 0;

        if (_is) {
            //len = arrlen / 2 + arrlen % 2;
            if(arrlen % 2>0){
                len = parseInt(arrlen / 2)+1;
            }else{
                len = parseInt(arrlen / 2);
            }
        } else {
            //len = arrlen / 5 + arrlen % 5;
            if(arrlen % 5 >0){
                len = parseInt(arrlen / 5)+1;
            }else{
                len = parseInt(arrlen / 5);
            }
        }

        var j = 0;
        for (var i = 0; i < len; i++) {
            var ihtml = '';
            if (_is) {
                ihtml = '<li>' + arr[j] + '</li><li>' + arr[++j] + '</li>';
            } else {
                ihtml = '<li>' + arr[j] + '</li><li>' + arr[++j] + '</li><li>' + arr[++j] + '</li><li>' + arr[++j] + '</li><li>' + arr[++j] + '</li>';
            }
            ahtml += '<div class="swiper-slide"><ul class="m_pic clear">' + ihtml + '</ul></div>';
            j++;
        }
        _this.find('.swiper-wrapper').html(ahtml);
    }


    function changeSwiper1(obj) {
        var _width = jQuery(window).width();
        var _is = _width <= webWidth;
        var _this = jQuery(obj);
        var arr = new Array();
        var ahtml = '';
        _this.find('li').each(function (e) {
            arr[e] = jQuery(this).html();
        });
        var arrlen = arr.length;
//		alert(arrlen);
        var len = 0;
        if (_is) {
            if(arrlen % 2>0){
                len = parseInt(arrlen / 2)+1;
            }else{
                len = parseInt(arrlen / 2);
            }
        } else {
            if(arrlen % 8 >0){
                len = parseInt(arrlen / 8)+1;
            }else{
                len = parseInt(arrlen / 8);
            }
        }

        var j = 0;
        for (var i = 0; i < len; i++) {
            var ihtml = '';
            if (_is) {
                ihtml = '<li>' + arr[j] + '</li><li>' + arr[++j] + '</li>';
            } else {
                ihtml = '<li>' + arr[j] + '</li><li>' + arr[++j] + '</li><li>' + arr[++j] + '</li><li>' + arr[++j] + '</li><li>' + arr[++j] + '</li><li>' + arr[++j] + '</li><li>' + arr[++j] + '</li><li>' + arr[++j] + '</li>';
            }
            ahtml += '<div class="swiper-slide"><ul class="clear">' + ihtml + '</ul></div>';
            j++;
        }
        _this.find('.swiper-wrapper').html(ahtml);
    }


    changeSwiper1('.xfswiper0');
    changeSwiper1('.xfswiper1');
    changeSwiper1('.xfswiper2');
    changeSwiper1('.xfswiper3');
    changeSwiper1('.xfswiper4');
    changeSwiper1('.xfswiper5');
    changeSwiper1('.xfswiper6');
    changeSwiper('.swiper3');
    changeSwiper('.swiper4');
    changeSwiper('.swiper5');
    changeSwiper('.swiper6');
    changeSwiper('.swiper7');
    jQuery(window).resize(function () {
        changeSwiper1('.xfswiper0');
        changeSwiper1('.xfswiper1');
        changeSwiper1('.xfswiper2');
        changeSwiper1('.xfswiper3');
        changeSwiper1('.xfswiper4');
        changeSwiper1('.xfswiper5');
        changeSwiper1('.xfswiper6');
        changeSwiper('.swiper3');
        changeSwiper('.swiper4');
        changeSwiper('.swiper5');
        changeSwiper('.swiper6');
        changeSwiper('.swiper7');
    });
    var swiper3 = new Swiper('.swiper3', {
        nextButton: '.swiper-button-next3',
        prevButton: '.swiper-button-prev3',
        spaceBetween: 30
    });
    var swiper4 = new Swiper('.swiper4', {
        nextButton: '.swiper-button-next4',
        prevButton: '.swiper-button-prev4',
        spaceBetween: 30
    });
    var swiper5 = new Swiper('.swiper5', {
        nextButton: '.swiper-button-next5',
        prevButton: '.swiper-button-prev5',
        spaceBetween: 30
    });
    var swiper6 = new Swiper('.swiper6', {
        nextButton: '.swiper-button-next6',
        prevButton: '.swiper-button-prev6',
        spaceBetween: 30
    });
    var swiper7 = new Swiper('.swiper7', {
        nextButton: '.swiper-button-next7',
        prevButton: '.swiper-button-prev7',
        spaceBetween: 30
    });

    jQuery(function() {
        jQuery(".closed").click(
                function(){
                    jQuery(".topimg").slideUp();
                });

        // 收缩展开效果
        jQuery(".adminico").click(function(){
            jQuery(".adminmenu").animate({height: 'toggle', opacity: 'toggle'}, "slow");

        },function(){
            jQuery(".adminmenu").animate({height: 'toggle', opacity: 'toggle'}, "slow");
        });


        jQuery(".a_open").click(
                function(){
                    jQuery(this).hide();
                    jQuery(".aside_lef").slideDown(500);
                    jQuery(".fc_black").show();
                    jQuery(".a_closed").show();
                });
        jQuery(".a_closed").click(
                function(){
                    jQuery(this).hide();
                    jQuery(".aside_lef").hide();
                    jQuery(".fc_black").hide();
                    jQuery(".a_open").show();
                });
        jQuery(".fc_black").click(
                function(){
                    jQuery(this).hide();
                    jQuery(".aside_lef").hide();
                    jQuery(".fc_black").hide();
                    jQuery(".a_open").show();
                    jQuery(".a_closed").hide();
                });
        jQuery(".rm_pank .pk_year a").each(function(index){
            jQuery(this).mouseover(
                    function(){
                        jQuery(".rm_pank .pk_year a").removeClass("current");
                        jQuery(this).addClass("current");
                        jQuery(".rm_pank .rm_main .rm_con").hide();
                        jQuery(".rm_pank .rm_main .rm_con:eq(" + index + ")").show();
                    })
        });

        var headHeight=$(".head_top").height();
        var nav=$("#navf");
        $(window).scroll(function(){
            if($(this).scrollTop()>headHeight){
                nav.addClass("navFix");
            }
            else{
                nav.removeClass("navFix");
            }
        })

        var headHeight2=$(".topimg").height();
        var nav2=jQuery("#topnavf");
        jQuery(window).scroll(function(){
            if($(this).scrollTop()>headHeight2){
                nav2.addClass("topnavFix");
            }
            else{
                nav2.removeClass("topnavFix");
            }
        });

        var week = new Date().getDay();
        jQuery(".xf_news .pk_time a").removeClass("current");
        jQuery(".xf_news .time_pic .time_con").hide();
        if (week != 0) {
            jQuery(".xf_news .pk_time a:eq(" + (week - 1) + ")").addClass("current");
            jQuery(".xf_news .time_pic .time_con:eq(" + (week - 1) + ")").show();
            new Swiper(".xfswiper"+(week - 1), {
                nextButton: '.xfswiper-button-next'+(week - 1),
                prevButton: '.xfswiper-button-prev'+(week - 1),
                spaceBetween: 30
            });
        } else {
            jQuery(".xf_news .pk_time a:last").addClass("current");
            jQuery(".xf_news .time_pic .time_con:last").show();
            new Swiper(".xfswiper7", {
                nextButton: '.xfswiper-button-next7',
                prevButton: '.xfswiper-button-prev7',
                spaceBetween: 30
            });
        }

        jQuery(".xf_news .pk_time a").each(function(index){
            jQuery(this).click(function(){
                jQuery(".xf_news .pk_time a").removeClass("current");
                jQuery(this).addClass("current");
                jQuery(".xf_news .time_pic .time_con").hide();
                jQuery(".xf_news .time_pic .time_con:eq(" + index + ")").show();
                new Swiper(".xfswiper"+index, {
                    nextButton: '.xfswiper-button-next'+index,
                    prevButton: '.xfswiper-button-prev'+index,
                    spaceBetween: 30
                });
            });
        });

    });
</script>
