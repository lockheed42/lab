    function closeInfo() {
        userInfo.style.display = "none"
    }

    function userContent(b) {
        if (/@(.*)：/g.test(b)) {
            var a = /@(.*)：/g.exec(b);
            a = b.replace(a[0], '<span style="color:#00a1d6;cursor:pointer">' + a[0] + "</span>");
            return a
        } else {
            return b
        }
    }
