                            $(".tabs").append('<li>' + jstart + '-' + jend + '</li>');
                        }

                        jQuery(".tab_conbox a").each(function(i) {
                            jQuery(".tab_conbox a").slice(i * 50, i * 50 + 50).wrapAll('<li class="tab_con"></li>');
                        }); $.jqtab("#tabs", "#tab_conbox", "click", zindex);
                    });
