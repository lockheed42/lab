            var j = $(".bb-comment .comment-send .comment-emoji");
            var i;

            function g() {
                a.click(function(l) {
                    l.stopPropagation()
                });
                var k;
                j.click(function(l) {
                    l.stopPropagation();
                    a.css("top", $(this).position().top + 30);
