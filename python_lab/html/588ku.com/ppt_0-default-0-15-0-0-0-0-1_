					var headUrl = $('.user-drop-control .user-icon').attr('data-url');					
					var html = '<img src="'+headUrl+'" width="48" height="48" />';
					$('.user-drop-control .user-icon').append(html);
					var html = '<img class="head-pic" src="'+headUrl+'" width="48" height="48" />';
					$('.user-drop-control .user-info-img').append(html);
				}
			});
		</script></body></html> 