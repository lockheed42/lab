        $('.buy_sure_pay').on("click",function() {
            var  globaluid = $('#user_id').val();
            var beizhu=$('#beizhu').val();
            if(globaluid!=0){
                var data=collectData();
                var is_xg='';
                var arr=[28,29,30,31,32,33,34];
                var gid=parseInt(data.goodid);
                var num=parseInt(data.num);
                var panduan=$.inArray(gid,arr);
                if(num>=3 && panduan!=-1){
                    alert('活动期间限购2件月饼产品');
                    return false;
                }else if(panduan!=-1 && is_xg==1 ){
                    alert('活动期间限购2件月饼产品');
                    return false;
                }
                $.ajax({
                    url:'/index.php?m=Shop&a=ajax',
                    dataType:'json',
                    data:{type:'score',tprice:data.tPrice,uid:globaluid,address_id:data.address_id,ctime:data.time,product_id:data.product_id,num:data.num,price:data.price,goodName:data.goodName,goodId:data.goodid,beizhu:beizhu},
                    success:function(response){
                        if(response.status==1){
                            $.ajax({
                                url:'http://ajax.588ku.com/?m=AjaxShop&a=totalorder',
                                dataType:'json',
                                success:function(response){
                                }
                            });
                            //交易成功 跳转到 订单详细；
                            $('#fill-makesure-wd').parent().attr('style','display:none')
                            if(confirm('交易成功是否前往订单页')){
                                window.location.href = 'http://588ku.com/?m=Shop&a=orderList';
                            }else{
                                window.location.href = 'http://588ku.com/?m=Shop&a=goodsDetail&goodsid='+data.goodid +'';
                            }
                        }else if(response.status==3){
                            //订单成功
                            $('#fill-makesure-wd').parent().attr('style','display:none')
                            $('#notfull').text(response.abs);
                            $('#fill-notfull-wd').parent().attr('style','display:block')
//                            if(confirm('订单成功，但是千库币不足,是否前往订单页')){
//                                window.location.href = 'http://588ku.com/?m=Shop&a=orderList';
//                            }else{
//                                $('#fill-notfull-wd').parent().attr('style','display:block')
//                            }
                        }else{
                        //系统错误输出信息  刷新页面
                            alert(response.message);
                            window.location.href = 'http://588ku.com/?m=Shop&a=goodsDetail&goodsid='+data.goodid +'';
                        }
                    }
                });
            }else{
                Login.phoneLogin();
            }})
        //通过库存判断状态
        function initStatus(){
            var quantity=$('.quantity').html();
            quantity=parseInt(quantity);
            if(quantity>0){
                $('.browse-oos').attr('style','display:none');
                $('.reserve').attr('style','display:none');//隐藏我要预订;
                $('.buy').removeClass('oos');////

            }else{
                $('.browse-oos').attr('style','display:block;font-size:18px');
                $('.reserve').attr('style','display:block');
                $('.buy').addClass('oos');
                $('.buy').attr("disabled",true);
            }
        }

        function collectData(){
            var attribute='';
            var goodName=$('#biaoti').text();
            var attributeName='';
            var price=parseInt($('.price .number').text());
            var amount=parseFloat($('.price .amount').text());
            var number=parseInt($('.num-control').children('input:first').val());
            var totalPrice=price*number;
            var totalamount=amount*number;
            var timestamp=(new Date().getTime().toString()).substr(0,10);
            var goodsid = $('#goods_id').val();
            var address_id=$('#gwj-aid').val();
            var product_id=$('#product_id').val();

            $('.attribut').each(function(){
                if($(this).hasClass('on')){
                    attribute+=$(this).attr('data-kid')+",";
                    attributeName+=$(this).text()+"\t";
                }
            })
            if (attribute.length > 0) {
                attribute = attribute.substr(0,attribute.length - 1);
            }
            return {goodName:goodName,attr:attribute,attName:attributeName,goodid:goodsid,num:number,time:timestamp,tPrice:totalPrice,price:price,address_id:address_id,product_id:product_id,amount:amount,totalamount:totalamount}
        }
        //判断库存
        function quantityStatus(){
            var  quantity=$('.quantity').html();
            var number=parseInt($('.num-control').children('input:first').val());
            quantity=parseInt(quantity);
            if(quantity==0){
                return false;
            }else if(quantity < number ){
                alert('备货不足哦');
                return false;
            }else{
                return true;
            }

        }
        function setRegion(currId, currLevel)
        {
            //发出ajax请求
            $.ajax({
                url:'/index.php?m=Shop&a=ajax',
                dataType:'json',
                data:{type:'region',region_id:currId},
                success:function(response){
                    if(response.status==1){
                        // 确定下一级的select
                        var nextLevel = currLevel + 1;
                        var nextSelect = $('.fill-address-wd  select[level=' + nextLevel + ']');
                        // 初始化选项中已有的数据
                        nextSelect.find('option[value!="-1"]').remove();
                        // 遍历所有的地区数据
                        for(var i = 0; i < response.list.length; ++i) {
                            var currRegion = response.list[i];
                            // 将地区, 形成option, 放在下级的select中
                            nextSelect.append('<option value="'+currRegion.id+'">' + currRegion.title + '</option>');
                        }
                    }else{
                        console.error('地区数据有错误');
                        return false ;
                    }
                }
            });
        }


        $('.buy').on("click",function() {
                var data=collectData();
                if(data.attr==''){
                     alert('请选择属性');
                    return false;
                 }
                 data=collectData();
                if(globaluid!=''){
                    var is_xg='';
                    var arr=[28,29,30,31,32,33,34];
                    var gid=parseInt(data.goodid);
                    var num=parseInt(data.num);
                    var panduan=$.inArray(gid,arr);
                    if(num>=3 && panduan!=-1){
                        alert('活动期间限购2件月饼产品');
                        return false;
                    }else if(panduan!=-1 && is_xg==1 ){
                        alert('活动期间限购2件月饼产品');
                        return false;
                    }
                    if(quantityStatus()){
                        var addres=$('#address_id').val();
                        if(addres==''){
                            $('.fill-address-wd').parent().attr('style','display:block')
                            setRegion(1, 0);
                        }else{
                            $('.buy-name').html(data.goodName);
                            $('.buy-attr').html(data.attName);
                            $('#buy_number').html(data.num);
                            $('#buy_price').html(data.price);
                            if(!isNaN(data.amount)){
                                $('#buy_price').parent().find('span').html('/&nbsp;<b  style="color:#e70000">'+data.amount+'</b>元');
                            }
                            if(!isNaN(data.totalamount)){
                                $('#buy_totalprice').parent().find('span').html('/<b  style="color:#e70000">'+data.totalamount+'</b>元');
                            }
                            $('#buy_totalprice').html(data.tPrice);
                            $('#sure_totalprice').html(data.tPrice);
                            $('#fill-order-wd').parent().attr('style','display:block')
                        }
                    }else{
                        return false;
                    }
              }else{
                  Login.phoneLogin();
              }
            })
        //这里是确认支付千库币的function
        $('.buy_pay').on("click",function(){
            var that=$('#fill-order-wd').parent();
            var style=$(this).attr('data-style');

            var data=collectData();
           if(style==3){
               $('#fill-order-wd').parent().attr('style','display:none');
               //跳出确认支付页面
               $('#fill-makesure-wd').parent().attr('style','display:block');
             }else{
               if(isNaN(data.amount)){
                   alert('此产品暂时不支持现金支付');
                   return;
               }
               var mallWd = window.open();
               $.ajax({
                   type:'POST',
                   url:'/index.php?m=Shop&a=ajax',
                   dataType:'json',
                   data:{type:'anotherpay',totalamount:data.totalamount,product_id:data.product_id,num:data.num,address_id:data.address_id,style:style},
                   success:function(response){
                       if(response.status==1){
                           if(response.style==2){
                               mallWd.close();
                               that.hide();
                               $('#fill-makesure-wxcodeurl').parent().fadeIn(800);
                               $('#fill-makesure-wxcodeurl').parent().find('img').attr('src',response.url)
                               $('#order_list').val(response.order_no)
                                t1=setInterval(update_native_state,2000);

                               window.onblur =function(){
                                   clearInterval(t1);
                               };
                               window.onfocus =function(){
                                   clearInterval(t1);
                                   t1 =setInterval(function(){
                                       update_native_state();
                                   },2000);
                               }
                           }else{
                               mallWd.location = response.url;
                              window.location.reload();
                           }

                       }else{

                           //系统错误输出信息  刷新页面
                           alert(response.message);
//                           window.location.href = 'http://588ku.com/?m=Shop&a=goodsDetail&goodsid='+data.goodid +'';
                       }
                   }
               });
           }

        })
        $(".kuMall-payType li").on("click",function(){
            var style=$(this).attr('data-style');
            $(this).addClass("on").siblings().removeClass("on");
            $('.buy_pay').attr('data-style',style);
        });
        $('.close-btn-shop').on("click",function(){
            clearInterval(t1);
            $('.button-shipping-address').attr("data-save",'0');
            $('.down-limit').each(function(){
                $(this).attr('style','display:none')
            })
        })
        $('.reserve').on("click",function() {
            if(globaluid!=0){
                var datas=collectData();
                if($('.reserve').attr('data-attr')>1){
                    alert('已经预订过了')
                }else{
                    $.ajax({
                        url:'/index.php?m=Shop&a=ajax',
                        dataType:'json',
                        data:{type:'reserve',attr:datas.attr,goodid:datas.goodid},
                        success:function(data){
                            if(data.status == 0){
                                alert('新产品正在上线 敬请期待');
                                return false;
                            }else{
                              var product_id=data.product_id;
                              var number=parseInt($('.num-control').children('input:first').val());
                              var  globaluid = $('#user_id').val();

                                $.ajax({
                                    url:'/index.php?m=Shop&a=ajax',
                                    dataType:'json',
                                    data:{type:'reserveSave',pid:product_id,num:number,uid:globaluid},
                                    success:function(response){
                                        if(response.status==1){
                                            $('.reserve').addClass("suc").html('<em></em>'+response.message);
                                        }else{
                                            $('.reserve').addClass("suc").html('<em></em>'+response.message);
                                            $('.reserve').attr('data-attr','2');
                                        }
                                    }
                                });
                            }
                        }
                    });
                }
            }else{
                Login.phoneLogin();
            }})
//        $('#fill-makesure-wxcodeurl .close-btn-shop').on('click',function(){
//            $('.down-limit').each(function(){
//                $(this).attr('style','display:none')
//                clearInterval(t1);
//            })
//        })
        var t1;
        var sum=0;
        function update_native_state(){
            sum++;
            if(sum>600){
                window.clearInterval(t1);
                return false;
            }
            if(sum>180){
                m=sum % 10;
                if(m!=0){return false;}
            }
            var order = $("#order_list").val();
            $.ajax({url:'http://588ku.com/index.php?m=ShopPay&a=scanResponse',dataType:'json',type:'post',data:{order_no:order}})
                    .success(function(data){
                        if(data.status==1){
                            window.clearInterval(t1);
                            $('.wx-btn').hide();
                            $('.wx-btn .success').show();
                            timmer = setTimeout(function () {
                                window.location.href="http://588ku.com/index.php?m=Shop&a=orderList";
                            }, 3000);
                        }
                    })
        }

