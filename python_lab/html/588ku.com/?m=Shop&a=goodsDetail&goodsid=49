        function judgeInput(){

            var customer=$('.customer').val();//获取收货人名
            var phonenumber=$('.phone-input').val();
            var province_id=$('#input-province_id').val();
            var city_id=$('#input-city_id').val();
            var area_id=$('#input-area_id').val();
            var address=$('#input-address').val()
            var status=0;//状态吗

            if(customer.length<2){
                if(!$('.customer').next().attr('data-option')){
                    $('.customer').parent().append('<span data-option="0"><font color="red">' + '请填写收货人名字' + '</font></span>')
                }
                status=1;
            }
            if(phonenumber.length != 11){
                if(!$('.phone-input').next().attr('data-option')){
                    $('.phone-input').parent().append('<span data-option="0"><font color="red">' + '请填写正确手机号码' + '</font></span>')
                }
                status=1;
            }
            if(address.length <= 5){
                if(!$('#input-address').next().attr('data-option')){
                    $('#input-address').parent().append('<span data-option="0"><font color="red">' + '地址不详细' + '</font></span>')
                }
                status=1;
            }
            if(province_id==-1||city_id==-1){
                if(!$('#input-area_id').next().attr('data-option')){
                    $('#input-area_id').parent().append('<span data-option="0"><font color="red">' + '请填写大区信息' + '</font></span>')
                }
                status=1;
            }
            if( status==1){
                return false;
            }else{
                return true
            }
        }
        //初始化地址页面（清空）
        function initAddress(){
            $('.customer').val('')
            $('.phone-input').val('');
            $('#input-province_id').val(-1);
            $('#input-city_id').val(-1);
            $('#input-area_id').val(-1);
            $('#input-address').val('')
        }
        // 选择切换
        $('.button-shipping-address').on("click",function() {
            var customer=$('.customer').val();//获取收货人名
            var uid=$('#user_id').val();
            var phonenumber=$('.phone-input').val();
            var province_id=$('#input-province_id').val();
            var city_id=$('#input-city_id').val();
            var area_id=$('#input-area_id').val();
            var province=$('#input-province_id').find("option:selected").text();
            var city=$('#input-city_id').find("option:selected").text()
            var area=$('#input-area_id').find("option:selected").text()
            var address=$('#input-address').val()
            var allAddress=province+city+area+address;
            var is_defalt=1;
            var data=collectData()
                if($('#address').hasClass('select')){
                    is_defalt=0;
               }
               if(!judgeInput()){
                   return false
               }
                if($('.button-shipping-address').attr('data-save') >= '1'){
                        return false
               }

                $('.button-shipping-address').attr("data-save",'1');
                initAddress();
                $.ajax({
                    url: '/index.php?m=Shop&a=ajax',
                    dataType: 'json',
                    data: {type: 'address', username: customer,phone:phonenumber, province:province_id,city:city_id,area:area_id,address:address,option:is_defalt,user_id:uid,allAddress:allAddress},
                    success: function (response) {
                        if (response.status == 1) {
                            $('.button-shipping-address').attr("data-save",'2');
                            $('#fill-address-wd').parent().attr('style','display:none')
                            $('#gwj-aid').val(response.address);
                            $('#address-shop').html('<a href="javascript:;" target="_blank" data-aid=" '+response.address+'"><em></em>'+customer+'&nbsp;&nbsp;'+phonenumber+'&nbsp;&nbsp;'+province+city+area+address+'</a>');
                            $('#address-shop').parent().find('.option').append('<a href="javascript:;" target="_blank" data-aid=" '+response.address+'"><em></em>'+customer+'&nbsp;&nbsp;'+phonenumber+'&nbsp;&nbsp;'+province+city+area+address+'</a>');



                            $('.buy-name').html(data.goodName);
                            $('.buy-attr').html(data.attName);
                            $('#buy_number').html(data.num);
                            $('#buy_price').html(data.price);
                            $('#buy_totalprice').html(data.tPrice);
                            $('#sure_totalprice').html(data.tPrice);


                            $('#fill-order-wd').parent().attr('style','display:block')
                        }else{
                            alert(response.message);
                            return false;
                        }
                    }
                });
        });
        $('#input-province_id,#input-city_id,#input-area_id').on("change",function() {
            $('#input-area_id').next().remove();
            var maxLevel = 3;// 支持的最大地区层级
            // 当前选择的地区的ID
            // this 当前事件源对象
            var currId = $(this).val();
            // 当前地区层级
            var currLevel = Number($(this).attr('level'));
            // 选择-1 不做任何操作, return 方法终止!
            if (currId == '-1') return ;
            // 当前是最后一级地区, 不需要ajax请求
            if (maxLevel == currLevel) return ;

            // 设置地区
            setRegion(currId, currLevel);
        });
