        function get_video_comment(v_id,page) {
            page = page || 1;
            v_id = v_id || 0;
            if (v_id == 0){
                return false;
            }
            $.get('/?m=Image&a=getComment',{v_id:v_id,page:page},function (res) {
                if (res.status == 1){
                    $('.user-CommentBox').html(res.data);
                    return true;
                }else{
                    return false;
                }
            },'json');

        }

